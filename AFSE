    local inviteLink = "https://discord.gg/aasNAxsT3P"
    setclipboard(inviteLink)
    
    Fluent:Notify({
        Title = "Discord Invite",
        Content = "Invite link copied to clipboard!\nJoin for updates & support!",
        Duration = 8
    })
end)

